  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.3",
    "@vercel/ncc": "^0.36.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@ltd/j-toml": "^1.37.0",
    "is-reachable": "^5.2.1",
    "node-pty": "^0.10.1",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2"
  }
}
