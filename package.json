{
  "name": "ocs2-prolinuxd",
  "version": "1.0.0",
  "description": "Prolinux Daemon for OCS2",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/index.ts",
    "build_app": "ncc build app-src/prolinux-config.ts --v8-cache -m -o dist/prolinux-config",
    "build": "ncc build session-wrapper-src/wrapper.ts --v8-cache -m -o dist/session-wrapper && ncc build src/index.ts --v8-cache -m -o dist/ && npm run build_app"
  },
